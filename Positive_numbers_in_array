// Create a function that receives an array of numbers as an argument and returns an
array containing only the positive numbers

const numbers = [-1, -5, -6, 2, 3, 4, 7 ]
let positive = []
for (let i = 0; i < numbers.length; i++) {
  if (numbers[i] > 0){
    positive.push(numbers[i])
  }
}
console.log(positive)
