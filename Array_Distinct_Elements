// Create a function that will receive an array of numbers as argument and will return a new array with distinct elements

function arraydistinct (arr1){
  const result = [];
  for(const num of arr1){
    if(!result.includes(num)){
      result.push(num);
    }
  }
  return result;
}

const arr1 = [1,1,1,5,5,6,7,7,7,8,8,8,9,9,9]


const arrays = arraydistinct(arr1);

console.log(arrays)
